<div class="media">
  <%= render partial: 'votes/voter', locals: { page: page } %>
  <div class="media-body">
    <h4 class="media-heading">
      <%= link_to page.title, page_path(page) %>
    </h4>
    <small>
      updated <%= time_ago_in_words(page.updated_at) %> <br>
    </small>
  </div>
</div>
